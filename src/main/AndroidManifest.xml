<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jinhyeokfang.androidproject.sunrininfra">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity
            android:name="com.jinhyeokfang.androidproject.SunRinInfra.SplashActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.jinhyeokfang.androidproject.SunRinInfra.MapActivity"
            android:label="선린 학교 지도"/>
        <activity
            android:name="com.jinhyeokfang.androidproject.SunRinInfra.MainActivity"
            android:label="@string/app_name"/>
        <activity
            android:name="com.jinhyeokfang.androidproject.SunRinInfra.LoginActivity" />
        <activity
            android:name="com.jinhyeokfang.androidproject.SunRinInfra.SignupActivity" />
        <activity
            android:name="com.jinhyeokfang.androidproject.SunRinInfra.FindAccountActivity" />
        <activity
            android:name="com.jinhyeokfang.androidproject.SunRinInfra.ChangePasswordActivity" />
    </application>

</manifest>